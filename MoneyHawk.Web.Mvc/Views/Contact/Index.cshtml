@using MoneyHawk.Core
@using Net.Web
@model System.Collections.Generic.IEnumerable<MoneyHawk.Core.Contact>

@{
    ViewBag.Title = "Contacts";
}

@{
    var grid = new WebGrid<Contact>(source:Model); 
}

<div id="grid">
    @grid.GetHtml(
        tableStyle: "table table-striped table-bordered",
        headerStyle: "head",
        alternatingRowStyle: "table-striped"
    )
</div>